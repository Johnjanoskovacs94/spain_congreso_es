<td valign="top" style="padding:0px 0px 0px 0px;width:100%;"><div id="p73_1333155_73_1333154_1333154"><table border="0" width="100%" cellpadding="0" cellspacing="0" class="RegionNoBorder"><tbody><tr><td class="RegionHeaderColor" width="100%" dir="LTR"><div id="pcnt73_1333155_73_1333154_1333154">











        
    
























 





    






       



       




    


   
     
   
<script language="JavaScript" type="text/javascript">

    function mostrarIniciativas(){
    
        document.getElementById( "capa_intervenciones").style.display = "none";
        if(document.getElementById("capa_iniciativas") != null ){    
            if(document.getElementById("capa_iniciativas").style.display == 'none'){
                document.getElementById("capa_iniciativas").style.display = 'block';  
            } else {
                document.getElementById("capa_iniciativas").style.display = 'none';    
            }
        }
    }
    
    function mostrarIntervenciones(){
    
        document.getElementById("capa_iniciativas").style.display = "none";
        if(document.getElementById("capa_intervenciones")!= null ){    
            if(document.getElementById("capa_intervenciones").style.display == 'none'){
                document.getElementById("capa_intervenciones").style.display = 'block';  
            } else {
                document.getElementById("capa_intervenciones").style.display = 'none';    
            }
        }
    }
    
    function noElegido( capaElegida ){            
        
        if( document.getElementById( capaElegida ) == document.getElementById( "spanIniciativas" ) ){            
            document.getElementById( "spanIntervenciones" ).className = "noElegido";
            document.getElementById( "spanIniciativas" ).className = "elegido";            
        } else {                            
            document.getElementById( "spanIniciativas" ).className = "noElegido";
            document.getElementById( "spanIntervenciones" ).className = "elegido";
        }
    
    }
    
    function activarEnlace(elemento){
 
        var elem = document.getElementById(elemento);                  
        elem.style.color='#FFFFFF';
        elem.style.text='none';
        elem.style.background='#9C1E3D';
  }
  
   function desactivarEnlace( elemento ){
        
        var elem = document.getElementById(elemento);     
        
        elem.className = "";
        elem.style.color='#000000';
        elem.style.text='';
        elem.style.background='#F3F3F3';
       
  }
  
  function cambiarEstilos( elemento, tipo ){
  
    if( tipo == 'iniciativas' ){
        var meses = new Array(  "enlaceUltimas", "enlaceOrales", "enlaceEscritas", "enlaceInformes", "enlaceOrganos", "enlaceTodas" );
    } else if ( tipo == 'iniciativasAnteriores' ){
        var meses = new Array(  "enlaceOrales", "enlaceEscritas", "enlaceInformes", "enlaceOrganos", "enlaceTodas" );
    } else if ( tipo == 'intervenciones' ){
        var meses = new Array(  "enlaceUltimas", "enlaceTipo", "enlaceCrono" );
    } else {
        var meses = new Array(  "enlaceTipo", "enlaceCrono" );
    }

    for( i = 0; i < meses.length; i++ ){
        
        if( elemento == meses[i] ){                    
            activarEnlace( meses[i] );
        } else {                    
            desactivarEnlace( meses[i] );
        }                
    }          
  }
  
  
  
  function letrasBlancas( elemento ){
  
    var elemento = document.getElementbyId( elemento );
    elemento.style.color = "#FFFFFF";
    
  }
        
</script>
